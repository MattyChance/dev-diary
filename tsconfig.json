{
    "compilerOptions": {
      "outDir": "./built",
      "allowJs": true,
      "target": "es6",
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node"
    },
    "lib": [
      "es2016",
      "dom",
      "es5",
      "es6",
      "esnext"
    ],
    "include": ["./src/**/*"],
    "exclude": [
      "node_modules",
  ]
}
